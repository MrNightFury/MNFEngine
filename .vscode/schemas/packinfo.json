{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "name": {
            "type": "string"
        },
        "defaultScene": {
            "type": "string"
        },
        "authors": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "backgroundMusic": {
            "type": "string"
        },

        "pfv": {
            "type": "number"
        },
        "modules": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    },
    "required": ["name", "defaultScene"]
}